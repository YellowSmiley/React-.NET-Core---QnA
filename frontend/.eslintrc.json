{
    "extends": ["react-app","prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", {
           "endOfLine":"auto",
           "printWidth": 80,
            "singleQuote": true,
            "semi": true,
            "tabWidth": 2,
            "useTabs": false
         }]
    }
}